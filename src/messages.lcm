package robot;

struct dwm_position_t
{
        int64_t timestamp; // in milliseconds
        float x;
        float y;
        float z;
        int8_t q;
}

struct dwm_acceleration_t
{
        int64_t timestamp; // in milliseconds
        float x;
        float y;
        float z;
}

struct vector2d_t
{
        int32_t x;
        int32_t y;
        double rad;
}

struct action_t
{
        string action;
        string pos;
}